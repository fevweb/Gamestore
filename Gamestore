DROP TABLE PROMOTION CASCADE CONSTRAINTS PURGE;
DROP TABLE GAME_DEVELOPER CASCADE CONSTRAINTS PURGE;
DROP TABLE ITEMS CASCADE CONSTRAINTS PURGE;
DROP TABLE GAME CASCADE CONSTRAINTS PURGE;
DROP TABLE DEVELOPER CASCADE CONSTRAINTS PURGE;
DROP TABLE GAME_PUBLISHER CASCADE CONSTRAINTS PURGE;
DROP TABLE GAME_ORDER CASCADE CONSTRAINTS PURGE;
DROP TABLE CUSTOMER CASCADE CONSTRAINTS PURGE;

CREATE TABLE CUSTOMER
(CustomerID  NUMBER(4) NOT NULL CONSTRAINT PK_CUSTOMER PRIMARY KEY,
LastName VARCHAR2(60) NOT NULL,
FirstName VARCHAR2(60) NOT NULL,
Address VARCHAR2(100) NOT NULL,
City VARCHAR2(50) NOT NULL,
State CHAR(2) NOT NULL,
Region VARCHAR2(50) NULL,
Country VARCHAR2(50) NULL,
Zip VARCHAR2(5) NOT NULL,
email VARCHAR2(50) NULL,
Gender VARCHAR2(10) NULL,er
MaritalStatus VARCHAR2(50) NULL,
Referred NUMBER(4) NULL);




