CREATE TABLE ITEMS
(OrderID NUMBER(4) NOT NULL CONSTRAINT FK_GAME_ORDER_ITEMS REFERENCES GAME_ORDER(OrderID)ON DELETE CASCADE,
ItemNum NUMBER(4) NOT NULL,
GameID NUMBER(6) NOT NULL CONSTRAINT FK_GAME_ITEMS REFERENCES GAME(GameID) ON DELETE CASCADE,
Quantity NUMBER(3) NOT NULL, 
CONSTRAINT PK_ITEMS PRIMARY KEY(OrderID, GameID));

