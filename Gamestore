CREATE TABLE GAME_DEVELOPER
(GameID NUMBER(6) NOT NULL CONSTRAINT fk_GAME_DEVELOPER REFERENCES GAME(GameID) ON DELETE CASCADE ,
DeveloperID NUMBER(4) NOT NULL CONSTRAINT fk_DEVELOPER_GAME_DEVELOPER REFERENCES DEVELOPER(DeveloperID) ON DELETE CASCADE,
CONSTRAINT PK_GAME_DEVELOPER PRIMARY KEY (GameID,DeveloperID));

