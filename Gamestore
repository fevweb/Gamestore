SELECT  CustomerId, LastName, State
FROM CUSTOMER
WHERE State NOT IN('FL','CA','NY');


SELECT Game_PubName, Country
FROM Game_Publisher
WHERE Country IN('JP', 'CA', 'US');

SELECT OrderID, ShipCity
FROM GAME_ORDER
WHERE ShipCity NOT IN('CHICAGO', 'TRENTON', 'TORONTO'); 


SELECT BarCode, Title, GameID
FROM GAME
WHERE GameID IN(5004,5010,5013);

SELECT  GameID, DeveloperID
FROM GAME_DEVELOPER
WHERE DeveloperID NOT IN(9100, 2105, 7100);


SELECT CustomerID
FROM  CUSTOMER
WHERE CustomerID BETWEEN 1003 AND 1007;

SELECT CustomerID
FROM CUSTOMER
WHERE CustomerID NOT BETWEEN 1009 AND 1016;

SELECT CustomerID, LastName, Gender
FROM CUSTOMER
WHERE CustomerID BETWEEN 1005 AND 1010
AND Gender = 'M';

SELECT GameID, Title, Category
FROM GAME
WHERE GameID NOT BETWEEN 5005 AND 5010
AND Category = 'Action';

SELECT ShipName, OrderID, OrderDate, ShipCountry
FROM GAME_ORDER
WHERE OrderDate BETWEEN '23-JAN-21' AND '30-JAN-21' AND ShipCountry ='US';

SELECT MaxRetailPrice
FROM PROMOTION
WHERE PromotionID  IN(7);

Ttitle "Transactions"
Btitle "End of Report"
SELECT * FROM PROMOTION;

SELECT CustomerID, LastName, FirstName
From CUSTOMER
WHERE LastName like 'M%';

SELECT CustomerID, LastName, FirstName
From CUSTOMER
WHERE LastName like '%H';

SELECT GameID, Title
From GAME
WHERE Title like '%dent%';

SELECT CustomerID, LastName, FirstName
From CUSTOMER
WHERE LastName like '%E_';

SELECT CustomerID, LastName, FirstName
From CUSTOMER
WHERE LastName like '__R%_';

SELECT CustomerID, LastName, FirstName
From CUSTOMER
WHERE LastName like 'G%D';

SELECT OrderID, OrderDate
FROM Game_Order
WHERE OrderDate LIKE '%DEC%';

SELECT GameID, Title
FROM GAME
WHERE GameID LIKE '5%';

SELECT GameID, Title
FROM GAME
WHERE GameID LIKE '%5';


6.	Create a report showing customerID and OrderID for all the orders made after January 1, 2021, that have been paid with a check and shipped within the US. 
	SELECT CustomerID, OrderID, OrderDate, PaymentMethod, ShipCountry
	FROM GAME_ORDER
	WHERE Order_Date > '01-Jan-21'
	AND(PaymentMethod = 'Check' AND ShipCountry = 'US');

7.	Create a report for each category with the Game's retail price, Title, Barcode, and for games that costs is equal or greater than 15 dollars.
	SELECT Category, RetailPrice, Title, Barcode, Cost
	FROM GAME
	WHERE Cost >= 15;

8.	Create a report that returns game ids and game developer ids that are not 9100, 2105, 7100
               SELECT GameID, DeveloperID
	FROM GAME_DEVELOPER
	WHERE DeveloperID NOT IN(9100, 2105, 7100);





SELECT Lname AS "Developer Last Name"
FROM DEVELOPER;

SELECT  CustomerID,OrderDate  AS "Customer Order Date"
FROM Game_Order;

SELECT ItemNum AS "Item Number"
FROM Items;

SELECT CustomerID, State
FROM CUSTOMER
WHERE State = 'FL'
INTERSECT 
SELECT CustomerID, ShipState
From Game_Order;

SELECT COUNT(*), CATEGORY
FROM  GAME
WHERE RetailPrice > 15
GROUP BY CATEGORY
HAVING COUNT(*) > 2;

SELECT ShipCountry, ShipCity, COUNT(*)
FROM GAME_ORDER
GROUP BY ShipCountry, ShipCity
HAVING COUNT(*) > 1;

SELECT SUM(COST * 0.05) AS "Game Cost"
FROM GAME
WHERE Cost > 20;

SELECT UPPER(Game_PubName)
From GAME_PUBLISHER;

SELECT LOWER(LastName), CustomerID
FROM Customer = 1001;


CREATE VIEW Customer_Info1 AS
SELECT FirstName, LastName,email
FRom CUSTOMER;


SELECT * FROM Customer_Info1;


CREATE VIEW Game_Info2 AS
SELECT GameID, Barcode, Title, Cost
FROM GAME
WHERE GameID BETWEEN 5005 AND 5010;

SELECT * FROM Game_Info2;



SELECT CUSTOMER.FirstName,CUSTOMER.LastName,GAME_ORDER.OrderDate
FROM CUSTOMER, GAME_ORDER
WHERE CUSTOMER.CustomerID = GAME_ORDER.CustomerID;


SELECT C.FirstName, C.LastName, GO.OrderDate
FROM CUSTOMER C, GAME_ORDER GO
WHERE C.CustomerID = GO.CustomerID;

SELECT ITEMS.QUANTITY,GAME.Cost,GAME.TITLE
FROM ITEMS, GAME
WHERE GAME.GAMEID = ITEMS.GAMEID
AND COST > 20;



SELECT GAME.Title,GAME.RetailPrice,ITEMS.Quantity, GAME_ORDER.OrderDate
FROM ITEMS
INNER JOIN GAME ON
GAME.GameID = ITEMS.GameID
INNER JOIN GAME_ORDER ON
GAME_ORDER.OrderID = ITEMS.OrderID
WHERE GAME_ORDER.OrderDate > '01-NOV-20';





SELECT CUSTOMER.CustomerID, CUSTOMER.LastName, GAME_ORDER.ShipCountry,ITEMS.Quantity
FROM CUSTOMER 
INNER JOIN GAME_ORDER ON
GAME_ORDER.CustomerID = CUSTOMER.CustomerID
INNER JOIN ITEMS ON
ITEMS.OrderID = GAME_ORDER.OrderID
WHERE GAME_ORDER.ShipCountry != 'US' AND ITEMS.Quantity > 1;



SELECT DEVELOPER.LName, DEVELOPER.FName, GAME.Title,  GAME_DEVELOPER.DeveloperID, GAME_PUBLISHER.GAME_PubName
FROM DEVELOPER
INNER JOIN GAME_DEVELOPER
ON GAME_DEVELOPER.DeveloperID = DEVELOPER.DeveloperID
INNER JOIN  GAME
ON GAME.GameID = GAME_DEVELOPER.GameID
INNER JOIN  GAME_PUBLISHER
ON GAME.GamePubID = GAME_PUBLISHER.GamePubID
WHERE GAME.Title ='FIFA';


SELECT  CustomerID, OrderDate, TO_CHAR(OrderDate,'MM/DD/YYYY') AS "Formatted Date"
FROM GAME_ORDER
WHERE CustomerID IN(1004,1008,1010);



SELECT  CustomerID, OrderDate, TO_CHAR(OrderDate,'DY') AS "Day 1",
  CustomerID, OrderDate, TO_CHAR(OrderDate,'Dy') AS "Day 2",
  CustomerID, OrderDate, TO_CHAR(OrderDate,'Month DD,YYYY') AS "Look at the Month",
  CustomerID, OrderDate, TO_CHAR(OrderDate,'HH:MI PM') AS "Time"
FROM GAME_ORDER
WHERE CustomerID IN(1004,1008,1010);

SELECT  CustomerID, OrderDate, TO_CHAR(OrderDate,'DY') AS "Day 1",
CustomerID, OrderDate, TO_CHAR(OrderDate,'Dy') AS "Day 2",
CustomerID, OrderDate, TO_CHAR(OrderDate,'Month DD, YYYY') AS "Look at the Month",
CustomerID, OrderDate, TO_CHAR(OrderDate,'HH:MI PM') AS "Time"
FROM GAME_ORDER
WHERE ShipCountry = 'US'

SELECT Title, (RetailPrice * 0.5) as "Commision"
FROM GAME
WHERE RetailPrice > 10;


SELECT GAME_DEVELOPER.DeveloperId, DEVELOPER.FName, DEVELOPER.LName, GAME.Title, (RetailPrice * 0.2) As " Game Royalty"
FROM GAME_DEVELOPER
INNER JOIN DEVELOPER ON
GAME_DEVELOPER.DeveloperID = DEVELOPER.DeveloperID
Inner Join GAME ON
GAME_DEVELOPER.GameID = GAME.GameID
Where GAME_DEVELOPER.DeveloperID = 5100;

SELECT CustomerID, LastName, FirstName, NVL(TO_CHAR(Referred), 'NOT Referred;') AS "Referred by"
FROM CUSTOMER
WHERE Referred IS NOT NULL;

CREATE VIEW CustInfo2_View AS
SELECT CustomerID, FirstName, LastName, State
FROM CUSTOMER;
SELECT * FROM CustInfo2_View;

CREATE VIEW CUST_ORDER AS
SELECT CUSTOMER.CustomerID,CUSTOMER.LastName,CUSTOMER.State, GAME_ORDER.OrderID,GAME_ORDER.OrderDate
FROM CUSTOMER
INNER JOIN Game_Order  ON
CUSTOMER.CustomerID =Game_Order.CustomerID;
SELECT * FROM CUST_ORDER;


SELECT GAME.Title, Count(*) AS "Number of authors"
FROM GAME_DEVELOPER
INNER JOIN GAME ON
GAME.GAMEID = Game_Developer.GAMEID
GROUP BY GAME.Title
HAVING COUNT(*) > 1;

SELECT ItemNum, Sum(Quantity) AS "Total", avg(Quantity) AS "Average", Min(Quantity) AS "Minimum", Max(Quantity) AS "Maximum"
FROM ITEMS
Group BY ItemNum;

SELECT LastName, City, State, Quantity AS "Number of games Purchased"
 FROM CUSTOMER
INNER JOIN Game_Order ON
CUSTOMER.CustomerID = GAME_ORDER.CustomerID
INNER JOIN ITEMS ON
GAME_ORDER.OrderID = ITEMS.OrderID
WHERE Quantity > 2;

SELECT GAME.Title, Game_Publisher.Game_PubName, Game_Publisher.ContactName, Game_Publisher.Phone
FROM GAME_PUBLISHER
INNER JOIN GAME ON
Game_Publisher.GamePubID  = GAME.GamePubID
Where Game_Publisher.Game_PubName Like 'R%';

SELECT DISTINCT Category
From GAME
Order by Category ASC;

SELECT Title, GamePubID AS "PublisherID"
FROM GAME;

Select Title, (RetailPrice-COST)  as "Price Markup"
FROM GAME;
