CREATE TABLE GAME_ORDER
(OrderID NUMBER(4) NOT NULL CONSTRAINT PK_GAME_ORDER_OrderID PRIMARY KEY,
CustomerID NUMBER(4) NOT NULL CONSTRAINT FK_GAME_ORDER_CUSTOMER REFERENCES CUSTOMER(CustomerID)ON DELETE CASCADE,
OrderDate DATE NOT NULL,
ShipDate DATE NULL,
ShipName VARCHAR2(50) NULL,
ShipStreet VARCHAR2(100) NOT NULL,
ShipCity VARCHAR2(50) NOT NULL,
ShipState VARCHAR2(50) NOT NULL,
ShipZip VARCHAR2(20) NOT NULL,
ShipCountry CHAR(2) NOT NULL,
PaymentMethod VARCHAR2(100) NOT NULL);

