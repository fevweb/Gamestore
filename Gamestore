CREATE TABLE GAME
(GameID NUMBER(6) NOT NULL CONSTRAINT PK_GAME PRIMARY KEY,
BarCode VARCHAR2(30) NOT NULL,
Title VARCHAR2(70) NOT NULL,
PubDate DATE NOT NULL,
GamePubID NUMBER(2) NOT NULL  CONSTRAINT FK_GAME_GAME_PUBLISHER REFERENCES GAME_PUBLISHER(GamePubID)ON DELETE CASCADE,
Cost NUMBER (7,2) NOT NULL,
RetailPrice NUMBER (7,2)  NULL,
Category VARCHAR2(40)  NULL);

